{
  "family": "aoc-FARGATE",
  "taskRoleArn": "{{task-role-arn}}",
  "executionRoleArn": "{{execution-role-arn}}",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "aoc-emitter",
      "image": "{{data-emitter-image}}",
      "essential": true,
      "entryPoint": [],
      "command": [],
      "environment": [
        {
          "name": "otlp_endpoint",
          "value": "localhost:55680"
        },
        {
          "name": "otlp_instance_id",
          "value": "aoc-sidecar-fargate-1598387155299"
        }
      ],
      "dependsOn": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-cwagent-sidecar-emitter",
          "awslogs-region": "{{region}}",
          "awslogs-stream-prefix": "ecs"
        },
      },
    },
    {
      "name": "aoc-collector",
      "image": "{{aoc-image}}",
      "portMappings": [
        {
          "containerPort": 55680,
          "hostPort": 55680,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": [],
      "command": [],
      "environment": [],
      "dependsOn": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ecs-cwagent-sidecar-collector",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        },
      },
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}